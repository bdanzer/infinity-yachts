{
    "name": "iyc/infinity-yachts",
    "version": "master",
    "description": "Plugin to help manage CYA feed",
    "type":"wordpress-plugin",
    "minimum-stability" : "stable",
    "authors": [
        {
            "name": "Britt Danzer",
            "email": "britt.danzer@gmail.com"
        }
    ],
    "extra": {
